{
  "QueryFilter" : 
    {
      "expression" : 
        {
          "operator" : "and",
          "nestedExpression": [
            {
              "argument" : ["${envId}"],
              "operator":"EQUALS",
              "property":"environmentId"
            },
            {
              "argument":["${processId}"],
              "operator":"EQUALS",
              "property":"processId"
            },
            {
              "argument":["${componentType}"],
              "operator":"EQUALS",
              "property":"componentType"
            }			
          ]
        }
    }
}
